#include <bits/stdc++.h>
using namespace std;
int findduplicate(vector<int> &nums)
{
    int n = nums.size();
    int slow=nums[0],fast=nums[nums[0]];
    int i=0;
    while(slow != fast)
    {
        slow = nums[slow];
        fast = nums[nums[fast]];
    }
    slow=0;
    while(slow!=fast)
    {
        slow = nums[slow];
        fast = nums[fast];
    }
    return nums[fast];
}
int main()
{
    vector<int> nums ={3,3,3,7,3};
    cout<<findduplicate(nums);

    return 0;
}
