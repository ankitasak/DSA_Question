https://leetcode.com/problems/find-the-k-beauty-of-a-number/description/
class Solution {
public:
    int divisorSubstrings(int num, int k) {
      string str = to_string(num);
        int cnt=0;
        for(int i = 0; i<=str.size()-k;i++) //ensures that the substring always has exactly k characters and never goes out of bounds.
        {
            string s1 = str.substr(i,k);
            int number1 = stoi(s1);
            if(number1 != 0){
                if(num % number1 == 0)
                {
                    cnt++;
                }
                
            }
        }
        return cnt;  
    }
};
