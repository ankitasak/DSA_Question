#include <bits/stdc++.h>
using namespace std;
bool matches(vector<int>,vector<int> );
 bool checkInclusion(string s1, string s2) {
        int len1 = s1.length();
        int len2 = s2.length();
        if(len1 > len2) return false;
        std::vector<int>  freq1(26,0),freq2(26,0);
        for(int i = 0;i < len1;i++)
        {
            freq1[s1[i]-'a']++;
            freq2[s2[i]-'a']++;
        }
        if(matches(freq1,freq2)) return true;
        for(int i = len1; i<len2 ; i++)
        {
            freq2[s2[i]-'a']++;
            freq2[s2[i-len1]-'a']--;
            if(matches(freq1,freq2)) return true;
        }
       
        return false;
    }
bool matches(vector<int> freq1,vector<int> freq2)
{
    for(int i =0;i<26;i++)
    {
        if(freq1[i]!=freq2[i]) return false;
    }
    return true;
}
int main()
{
   string s1,s2;
   cin>>s1>>s2;
bool val = checkInclusion(s1,s2);
if(val)
{
    cout<<"true";
}
else{
    cout<<"false";
}
    return 0;
}

-------------------
class Solution {
public:
    bool checkInclusion(string s1, string s2) {
        int len1 = s1.length();
        int len2 = s2.length();
        if(len1 > len2) return false;
        std::vector<int>  freq1(26,0),freq2(26,0);
        for(int i = 0;i < len1;i++)
        {
            freq1[s1[i]-'a']++;
            freq2[s2[i]-'a']++;
        }
        if(matches(freq1,freq2)) return true;
        for(int i = len1; i<len2 ; i++)
        {
            freq2[s2[i]-'a']++;
            freq2[s2[i-len1]-'a']--;
            if(matches(freq1,freq2)) return true;
        }
       
        return false;
    }
bool matches(vector<int> freq1,vector<int> freq2)
{
    for(int i =0;i<26;i++)
    {
        if(freq1[i]!=freq2[i]) return false;
    }
    return true;
}
};
