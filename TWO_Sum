#include <bits/stdc++.h>
using namespace std;
vector<int> twoSum(vector<int> &nums, int target)
{
    vector<int> result={};
    int n = nums.size();
    for(int i=0;i<n;i++)
    {
        
        for(int j=i+1;j<n;j++)
    {
        
        if(nums[i]+nums[j]==target)
        {
            result.push_back(i);
            result.push_back(j);
        }
    }
    }
    
    return result;
}
//using mapp
 vector<int> twoSum(vector<int>& nums, int target) {
        //create map 
        map<int,int>mapp_val;
        for(int i=0;i<nums.size();i++)
        {
            int ans= nums[i];
            int lookval=target-ans;
            if(mapp_val.count(lookval))
            {
                return {mapp_val[lookval],i};
            }
            mapp_val[nums[i]]=i;
        }
        return {-1,-1};
    }


int main()
{
   vector<int> arr={2,7,11,15};
   int target;
   cin>>target;
   std::vector<int> ans=twoSum(arr,target);
   for(auto itr : ans)
   {
       cout<<itr<<" ";
   }

    return 0;
}
