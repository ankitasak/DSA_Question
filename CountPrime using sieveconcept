#include <bits/stdc++.h>
using namespace std;
int countPrimes(int n)
{
    int cnt=0;
   if(n<=2)
   {
       return 0;
   }
   vector<int> prime_no(n,true);
   prime_no[0]=prime_no[1]=false;
   for(int i=2;i*i<n;i++)
   {
       if(prime_no[i])
      {
           for(int j =i*i;j<n;j+=i)
       {
         prime_no[j]=false;
       }
      }
   }
   for(auto ele:prime_no)
   {
       if(ele)
      { cnt++;}
   }
    return cnt;
}
int main()
{
    int n;
    cin>>n;
    cout<<countPrimes(n);

    return 0;
}
